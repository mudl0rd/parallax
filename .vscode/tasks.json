{
	"version": "2.0.0",
	"tasks": [
		{
			"windows": {
				"command": "C:\\msys64\\usr\\bin\\bash.exe",
				"args": [
					"--login", "-i",
					"./cli_build.sh"
				],
				"options": {
					"env": {
						"PATH": "C:\\msys64\\usr\\bin;C:\\msys64\\mingw64\\bin",
						"MSYSTEM": "MINGW64",
						"CHERE_INVOKING": "1",
						"MSYS2_PATH_TYPE": "inherit"
					},
				},
			},
			"linux": {
				"args": [
					"./cli_build.sh"
				],
				"command": "/bin/bash"
			},
			"options": {
				"env": {
					"LANG": "C.UTF-8",
					"PYTHONUNBUFFERD": "1",
					"MSYSTEM": "MINGW64",
					"CHERE_INVOKING": "1"
				}
			},
			"label": "build",
			"problemMatcher": {
				"owner": "cpp",
				"fileLocation": [
					"relative",
					"${workspaceRoot}/builddir"
				],
				"pattern": {
					"regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error|fatal error):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"windows": {
				"command": "C:\\msys64\\usr\\bin\\bash.exe",
				"args": [
					"--login", "-i",
					"./cli_build.sh"
				],
				"options": {
					"env": {
						"PATH": "C:\\msys64\\usr\\bin;C:\\msys64\\mingw64\\bin",
						"MSYSTEM": "MINGW64",
						"CHERE_INVOKING": "1",
						"MSYS2_PATH_TYPE": "inherit"
					},
				},
			},
			"linux": {
				"args": [
					"./cli_build.sh"
				],
				"command": "/bin/bash"
			},
			"options": {
				"env": {
					"LANG": "C.UTF-8",
					"PYTHONUNBUFFERD": "1",
					"MSYSTEM": "MINGW64",
					"CHERE_INVOKING": "1"
				}
			},
			"label": "builddbg",
			"problemMatcher": {
				"owner": "cpp",
				"fileLocation": [
					"relative",
					"${workspaceRoot}/builddir"
				],
				"pattern": {
					"regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error|fatal error):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5
				}
			},
			"group": {
				"kind": "none",
				"isDefault": true
			}
		},
	]
}